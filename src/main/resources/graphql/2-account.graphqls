type Account{
	id: ID!
	balance: Float!
	status: String!
	customer: Customer!
}

input AccountInput {
	id: ID
	balance: Float!
	status: String!
	customerID: ID!
}

extend type Query {
	account(id:ID!):Account
	accounts:[Account]
}

extend type Mutation {
	saveAccount(account: AccountInput!): Account!
	deleteAccount(id:ID!): Boolean!	
}
